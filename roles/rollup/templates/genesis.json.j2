{
  "accounts": {
    "accounts": [],
    "enable_custom_account_mappings": false
  },
  "operator_incentives": {
    "reward_address": "{{ rollup_sequencer_address }}"
  },
  "attester_incentives": {
    "minimum_attester_bond": {{ rollup_genesis_verifier_min_bond }},
    "minimum_challenger_bond": {{ rollup_genesis_challenger_min_bond }},
    "initial_attesters": [
      ["{{ rollup_sequencer_address }}", "{{ rollup_genesis_verifier_init_bond }}"]
    ],
    "rollup_finality_period": 5,
    "maximum_attested_height": 0,
    "light_client_finalized_height": 0
  },
  "bank": {
    "gas_token_config": {% if rollup_genesis_bank_disable_gas_token %}null{% else %}{
      "token_name": "sov-token",
      "address_and_balances": [
{%- set seq_has_balance = rollup_genesis_seq_init_balance|int != 0 %}
{%- set extras_have_balance = rollup_genesis_extra_init_balance|int != 0 and rollup_genesis_extra_addresses is not string and rollup_genesis_extra_addresses|length > 0 %}
{%- if seq_has_balance %}
        [
          "{{ rollup_sequencer_address }}",
          "{{ rollup_genesis_seq_init_balance }}"
        ]{% if extras_have_balance %},{% endif %}
{%- endif %}
{%- if extras_have_balance %}
{% for extra_address in rollup_genesis_extra_addresses %}
        [
          "{{ extra_address }}",
          "{{ rollup_genesis_extra_init_balance }}"
        ]{% if not loop.last %},{% endif %}
{% endfor %}
{%- endif %}
      ],
      "admins": {% if rollup_genesis_bank_disable_admin %}[]{% else %}["{{ rollup_sequencer_address }}"]{% endif %}
    }{% endif %},
    "tokens": []
  },
  "chain_state": {
    "current_time": 0,
    "operating_mode": "operator",
    "inner_code_commitment": [0, 0, 0, 0, 0, 0, 0, 0],
    "outer_code_commitment": [0, 0, 0, 0, 0, 0, 0, 0],
    "genesis_da_height": {{ genesis_da_height }}
  },
  "paymaster": {
    "payers": [
      {
        "payer_address": "{{ rollup_sequencer_address }}",
        "policy": {
          "default_payee_policy": {
            "allow": {}
          },
          "payees": [],
          "authorized_updaters": [],
          "authorized_sequencers": "all"
        },
        "sequencers_to_register": [
          "{{ da_rollup_address }}"
        ]
      }
    ]
  },
  "prover_incentives": {
    "proving_penalty": {{ rollup_genesis_proving_penalty }},
    "minimum_bond": {{ rollup_genesis_verifier_min_bond }},
    "initial_provers": [
      ["{{ rollup_sequencer_address }}", "{{ rollup_genesis_verifier_init_bond }}"]
    ]
  },
  "sequencer_registry": {
    "minimum_bond": "{{ rollup_genesis_seq_min_bond }}",
    "sequencer_config": {
      "seq_rollup_address": "{{ rollup_sequencer_address }}",
      "seq_da_address": "{{ da_rollup_address }}",
      "seq_bond": "{{ rollup_genesis_seq_init_bond }}",
      "is_preferred_sequencer": true
    }
  },
  "uniqueness": null,
  "blob_storage": null,
  "revenue_share": null,
  "mailbox": null,
  "interchain_gas_paymaster": null,
  "merkle_tree_hook": null,
  "warp": null,
  "value_setter": null,
  "evm": {
    "accounts": [],
    "contract_creation_policy": "everyone",
    "initial_base_fee": 7,
    "genesis_timestamp": 0,
    "chain_spec": {
      "limit_contract_code_size": 524288,
      "coinbase": "0x0000000000000000000000000000000000000000",
      "block_gas_limit": {{ rollup_genesis_evm_block_gas_limit }},
      "base_fee_params": {
        "max_change_denominator": 8,
        "elasticity_multiplier": 2
      },
      "hardforks": [[0, "CANCUN"]]
    }
  }
}